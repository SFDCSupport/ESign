{
    "name": "niit/esign",
    "description": "...",
    "keywords": [
        "esign"
    ],
    "type": "library",
    "authors": [
        {
            "name": "Anand Pilania",
            "email": "anand.pilania@niitmts.com",
            "role": "developer"
        }
    ],
    "require": {
        "php": ">=8.2",
        "illuminate/contracts": "^10.0",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.13"
    },
    "autoload": {
        "psr-4": {
            "NIIT\\ESign\\": "src/",
            "NIIT\\ESign\\Database\\Factories\\": "database/factories/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "NIIT\\ESign\\ESignServiceProvider"
            ],
            "aliases": {
                "ESign": "NIIT\\ESign\\ESignFacade"
            }
        }
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "beta",
    "prefer-stable": true,
    "archive": {
        "exclude": [
            "./node_modules",
            "./resources/html"
        ]
    }
}
